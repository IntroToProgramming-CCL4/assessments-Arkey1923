for a in range(0 , 51 , 1): #using range function to specify the parameters.
    print(a)

for b in reversed(ange(0 , 51, 1)): #using reversed function to reversed the counting.
    print(b)
    
for c in range (30 , 51 , 1):
    print(c)

for or d in reversed(range( 10 , 51, 2)): #counting with the increments of 2.
    print(d)

for e in range(100 , 201 , 5): #counting with the  increments of 5.
    print(e)
    
